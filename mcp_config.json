{
  "mcpServers": {
    "searxng-search": {
      "command": "uvx",
      "args": [
        "mcp-searxng"
      ],
      "env": {
        "SEARXNG_URL": "http://localhost:8080"
      }
    },
    "web-fetcher": {
      "command": "uvx",
      "args": [
        "scrapling-fetch-mcp"
      ],
      "env": {
        "HEADLESS_MODE": "true"
      }
    },
    "obsidian-vault": {
      "command": "npx",
      "args": [
        "-y",
        "@connorbritain/obsidian-mcp-server"
      ],
      "env": {
        "OBSIDIAN_API_KEY": "YOUR_OBSIDIAN_API_KEY",
        "OBSIDIAN_PROTOCOL": "https",
        "OBSIDIAN_VERIFY_SSL": "false"
      }
    },
    "ollama-bridge": {
      "command": "uvx",
      "args": [
        "mcp-ollama"
      ],
      "env": {
        "OLLAMA_BASE_URL": "http://localhost:11434",
        "OLLAMA_MODEL": "llama3.2:1b"
      }
    },
    "magic": {
      "command": "npx",
      "args": [
        "-y",
        "magic-mcp"
      ],
      "env": {}
    }
  }
}